{  
  "name":"QFS Support Link-2-Here",
  "description":"Link the closest anchor",
  "version":"1.1",
  "permissions":[  
    "activeTab",
    "contextMenus",
    "clipboardWrite",
    "storage"
  ],
  "content_scripts":[  
    {  
      "matches":[  
        "<all_urls>"
      ],
      "js":[  
        "content.js"
      ]
    }
  ],
  "background":{  
    "scripts":[  
      "background.js"
    ]
  },
  "options_page":"options.html",
  "options_ui":{  
    "page":"options.html",
    "open_in_tab":false
  },
  "icons":{  
    "16":"contextIcon.png"
  },
  "manifest_version":2
}